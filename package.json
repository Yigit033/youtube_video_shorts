{
  "name": "youtube-shorts-automation",
  "version": "1.0.0",
  "description": "Automated YouTube Shorts creation and upload platform using free tools",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node setup.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test-fixes": "node test-fixes.js",
    "docker:build": "docker build -t youtube-shorts-automation .",
    "docker:run": "docker run -p 3000:3000 youtube-shorts-automation",
    "docker:compose": "docker-compose up --build",
    "production:install": "npm install --production",
    "health:check": "curl -f http://localhost:3000/health || exit 1"
  },
  "dependencies": {
    "@huggingface/inference": "^4.7.1",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "say": "^0.16.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "google-auth-library": "^9.6.3",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.7.0",
    "openai": "^4.24.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  },
  "keywords": [
    "youtube",
    "automation",
    "shorts",
    "video",
    "ai",
    "tts",
    "ffmpeg"
